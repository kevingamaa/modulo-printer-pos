{
  "name": "electron-quick-start-typescript",
  "version": "1.0.0",
  "description": "A minimal Electron application written with Typescript",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm run build && electron ./dist/main.js",
    "dev": "nodemon --watch 'src/' --exec \"ts-node src/entry.ts\" -e ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electron/electron-quick-start-typescript.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "electron": "^9.4.0",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "@types/express": "^4.17.0",
    "body-parse": "^0.1.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "typescript": "^2.9.2"
  },
  "bugs": {
    "url": "https://github.com/electron/electron-quick-start-typescript/issues"
  },
  "homepage": "https://github.com/electron/electron-quick-start-typescript#readme",
  "main": "index.js"
}
